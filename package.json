    "faucet": "0.0.1",
    "jshint": "^2.5.10",
    "tape": "^3.0.3",
    "tape-run": "^0.3.0"
  },
  "dependencies": {
    "browser-launcher": "^1.0.0",
    "browser-run": "^0.4.0",
    "browserify": "^7.0.3",
    "ecstatic": "^0.5.8",
